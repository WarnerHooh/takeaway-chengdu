    "babel-register": "^6.5.2",
    "css-loader": "^0.23.1",
    "enzyme": "^1.6.0",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^2.1.1",
    "eslint-loader": "^1.2.1",
    "eslint-plugin-react": "^3.16.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "fetch-mock": "^4.1.0",
    "istanbul": "^1.0.0-alpha.2",
    "json-loader": "^0.5.4",
    "mocha": "^2.4.5",
    "node-libs-browser": "^1.0.0",
    "node-sass": "^3.8.0",
    "react-hot-loader": "^1.3.0",
    "react-proxy": "^1.1.2",
    "redux-devtools": "^3.1.1",
