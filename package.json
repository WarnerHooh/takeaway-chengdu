    "babel-polyfill": "^6.5.0",
    "flux-standard-action": "^0.6.0",
    "isomorphic-fetch": "^2.2.0",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-redux": "^3.1.0",
    "react-router": "^2.0.0",
    "react-router-redux": "^2.1.0",
